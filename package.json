{
  "name": "cookie-cloudflare",
  "version": "1.5.0",
  "description": "基于 hono 实现的兼容 CookieCloud API 的云函数服务，支持 Cloudflare Workers 部署",
  "author": "CaoMeiYouRen",
  "private": true,
  "license": "AGPL-3.0",
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "lint": "cross-env NODE_ENV=production eslint src --fix --ext .ts,.js,.cjs,.mjs",
    "release": "semantic-release",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production tsup",
    "dev": "cross-env NODE_ENV=development tsx watch src/index.ts",
    "dev:tsup": "cross-env NODE_ENV=development tsup --watch",
    "start": "node ./dist/index.mjs",
    "start:tsx": "cross-env NODE_ENV=production tsx src/index.ts",
    "start:bun": "bun ./dist/app.mjs",
    "commit": "cz",
    "prepare": "husky install",
    "dev:vercel": "vercel dev",
    "deploy:vercel": "vercel",
    "dev:wrangler": "wrangler dev --env dev",
    "deploy:wrangler": "wrangler deploy"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/crypto-js": "^4.2.2",
    "@types/debug": "^4.1.12",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^25.2.2",
    "@types/pako": "^2.0.4",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vercel/nft": "^1.3.0",
    "commitizen": "^4.3.1",
    "conventional-changelog-cli": "^5.0.0",
    "conventional-changelog-cmyr-config": "^3.0.0",
    "cross-env": "^10.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-conventional-changelog-cmyr": "^2.0.0",
    "debug": "^4.4.3",
    "eslint": "^8.57.1",
    "eslint-config-cmyr": "^1.1.32",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "rimraf": "^6.1.2",
    "semantic-release": "21.0.1",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vercel": "^37.9.0",
    "wrangler": "^4.63.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog-cmyr"
    }
  },
  "changelog": {
    "language": "zh"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run lint",
      "git add"
    ]
  },
  "keywords": [
    "cookie",
    "cloudflare",
    "hono",
    "Docker",
    "Node.js"
  ],
  "homepage": "https://github.com/CaoMeiYouRen/cookie-cloudflare#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CaoMeiYouRen/cookie-cloudflare.git"
  },
  "bugs": {
    "url": "https://github.com/CaoMeiYouRen/cookie-cloudflare/issues"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20260207.0",
    "@hono/node-server": "^1.19.9",
    "await-to-js": "^3.0.0",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.4",
    "fs-extra": "^11.3.3",
    "hono": "^4.11.8",
    "isomorphic-unfetch": "^4.0.2",
    "lodash-es": "^4.17.23",
    "pako": "^2.1.0",
    "tslib": "^2.8.1",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "pnpm": {
    "overrides": {
      "compare-func": "^2.0.0"
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "workerd"
    ]
  },
  "type": "module"
}
